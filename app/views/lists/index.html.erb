<div class="banner container-fluid">
<h1>Here are your lists!</h1>
</div>
<div class="container">

<ul>
  <% @lists.each do |l| %>
    <div class="card d-inline-flex"><li><%= link_to l.name, list_path(l) %></li></div>
  <% end %>
</ul>



<%= link_to 'Add a new list', new_list_path, class:'btn btn-success' %>
</div>
